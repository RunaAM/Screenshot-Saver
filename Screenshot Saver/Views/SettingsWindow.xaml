<Window x:Class="Screenshot_Saver.Views.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Screenshot_Saver.Views"
        xmlns:control="clr-namespace:Screenshot_Saver"
        mc:Ignorable="d"
        Title="SettingsWindow" Height="450" Width="800">
    
    <Grid>
        <Rectangle Fill="{StaticResource ControlBackground}"/>
        <Rectangle Style="{DynamicResource SettingItemFrame}"/>
        <control:ToggleSwitch Style="{DynamicResource ToggleSwitchStyle1}"/>
    </Grid>
</Window>
